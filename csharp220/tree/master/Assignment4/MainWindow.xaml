<Window x:Class="Assignment4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignment4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <TextBlock  Width="100">US Zip Code</TextBlock>
        <TextBox x:Name="USZipCode" TextChanged="USZipCode_TextChanged"></TextBox>
        <TextBlock Width="100">Canada Postal Code</TextBlock>
        <TextBox x:Name="CanadaPostal" TextChanged="CanadaPostal_TextChanged"></TextBox>
        <Button x:Name="uxsubmit" Width="50" Click="uxsubmit_Click">Submit</Button>
        
    </StackPanel>
</Window>
