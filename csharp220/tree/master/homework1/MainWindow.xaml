<Window x:Class="homework1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:homework1"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <TextBlock>Enter Name:</TextBlock>
        <TextBox x:Name="uxName" DataContextChanged="uxName_DataContextChanged" TextChanged="uxName_TextChanged"></TextBox>
        <TextBlock>Enter Password</TextBlock>
        <TextBox x:Name="uxPassword" TextChanged="uxPassword_TextChanged"></TextBox>
        <Button x:Name="uxSubmit" Click="uxSubmit_Click">Submit</Button>
    </StackPanel>
</Window>
